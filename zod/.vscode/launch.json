    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "zod deploy",
            "program": "${workspaceFolder}/bin/run",
            "args": [
                "deploy"
            ],
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "zod deploy:clean",
            "program": "${workspaceFolder}/bin/run",
            "args": [
                "deploy:clean",
                "--env=balls"
            ],
            "cwd": "${workspaceRoot}",
            "console": "integratedTerminal"
        }
    ]
}